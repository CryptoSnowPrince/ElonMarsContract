contract Elonmars is ERC721 {
    
    using Math for uint256;
    using Math for uint8;
    using Strings for uint8;

    IBEP20 public busd;

    uint256 public price = 30 ether;
    uint256 maxSupply = 1000;
    uint256 maxPerUser = 100;
    address receiver = 0x96Ca266261F828BAB32E800F5797f0eDc2cCE66f;
    // address receiver = 0xAb8483F64d9C6d1EcF9b849Ae677dD3315835cb2;
    address whitelist = 0x1cb6FC66926224EE12d4714a2A1E8F2ca509f0c1;
    address owner;

    uint256 public totalMints = 0;

    mapping(address => uint256) public balances;
    mapping(uint256 => uint8) public types;

    string[] public uris = [
        "https://ipfs.io/ipfs/QmP1n9sPJFkE5eVEhB3Vup364LHPJYCdufTxSjGhJPM5TL?filename=metadata_1.json",
        "https://ipfs.io/ipfs/QmVYjXC9NjrCGHw1jjFtVPJk8zgbabtuBoCgnkLXi9c6aL?filename=metadata_2.json",
        "https://ipfs.io/ipfs/QmcH9nMvPvgtR9Q1NPk5nNPW1BAoAqmDF7BZRdZBZYrEko?filename=metadata_3.json"
    ];

    constructor (string memory _name, string memory _symbol) ERC721(_name, _symbol) {
        busd = IBEP20(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56);
        
        owner = msg.sender;
    }
    
    modifier onlyOwner() {
        require(msg.sender == owner, "Not owner");

        _;
    }
    modifier validAddress(address _addr) {
        require(_addr != address(0), "Not valid address");
        _;
    }

    function changeOwner(address _newOwner) public onlyOwner validAddress(_newOwner) {
        owner = _newOwner;
    }

    function safeMint (address to, uint256 tokenId) internal {
        _safeMint(to, tokenId);
    }

    function _safeTransferFrom(
        IBEP20 token,
        address sender,
        address recipient,
        uint amount
    ) private {
        bool sent = token.transferFrom(sender, recipient, amount);
        require(sent, "Token transfer failed");
    }

    function mint (uint256 quantity, uint8 _type) external {
        
        require(balances[msg.sender] + quantity <= maxPerUser && quantity > 0, "mints per user exceeded");
        require(_type == 1 || _type == 2 || _type == 3, "ilegal type");

        if (msg.sender != whitelist) {
            uint256 amount = quantity * price * _type;

            uint256 allowance = busd.allowance(msg.sender, address(this));

            require(
                allowance >= amount,
                "busd allowance too low"
            );
            busd.approve(receiver, amount);

            _safeTransferFrom(busd, msg.sender, receiver, amount);
        }

        uint256 tokenId = totalMints;
        totalMints ++;

        safeMint(msg.sender, tokenId);
        balances[msg.sender] += quantity;
        types[tokenId] = _type;
    }

    function totalSupply () public view returns (uint256) {
        return totalMints;
    }

    function getBalance () public view returns (uint256) {
        return balances[msg.sender];
    }

    function tokenURI (uint256 tokenId) public view override returns (string memory){
        // return string.concat(baseURL, types[tokenId].toString(), ".json");
        return uris[types[tokenId] - 1];
    }

    function setTokenPrice (uint256 _price) external onlyOwner {
        require(_price > 0, "price should be greater than 0");

        price = _price;
    }

    function setMaxSupply (uint256 _maxSupply) external onlyOwner {
        maxSupply = _maxSupply;
    }

    function setMaxPerUser (uint256 _maxPerUser) external onlyOwner {
        maxPerUser = _maxPerUser;
    }
}